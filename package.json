{
  "name": "dexcom-mongo-backup-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/dustin-lennon/DexcomMongoBackup",
  "author": "Dustin Lennon <dustin.lennon@stelth2000inc.com>",
  "license": "UNLICENSE",
  "keywords": [
    "Mongo",
    "Dexcom",
    "Amazon S3"
  ],
  "scripts": {
    "build": "tsc -b src",
    "dev": "yarn build && yarn start",
    "watch": "tsc -b src -w",
    "watch:start": "tsc-watch -b src --onSuccess \"yarn start \"",
    "clean": "node scripts/build/clean.mjs",
    "start": "node --enable-source-maps dist/index.js",
    "update": "yarn upgrade-interactive",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "pm2-stop:dev": "pm2 stop dexcomMongoBot",
    "pm2-start:dev": "yarn clean && yarn build && pm2 start ./dist/index.js --name=dexcomMongoBot"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.369.0",
    "@aws-sdk/client-s3": "^3.369.0",
    "@aws-sdk/lib-storage": "^3.369.0",
    "@sapphire/decorators": "^6.0.1",
    "@sapphire/discord-utilities": "^3.0.3",
    "@sapphire/discord.js-utilities": "7.0.0",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "^4.5.0",
    "@sapphire/plugin-api": "^5.1.0",
    "@sapphire/plugin-editable-commands": "^3.0.2",
    "@sapphire/plugin-logger": "^3.0.4",
    "@sapphire/plugin-scheduled-tasks": "^7.1.0",
    "@sapphire/plugin-subcommands": "^4.0.1",
    "@sapphire/time-utilities": "^1.7.9",
    "@sapphire/type": "^2.4.0",
    "@sapphire/utilities": "^3.12.0",
    "@sentry/integrations": "^7.58.0",
    "@sentry/node": "^7.58.0",
    "@sentry/tracing": "^7.58.0",
    "@skyra/env-utilities": "^1.2.1",
    "@skyra/http-framework": "^1.0.3",
    "aws-sdk": "^2.1414.0",
    "discord.js": "^14.11.0",
    "filesize": "^10.0.7",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "mkdirp": "^3.0.1",
    "moment": "^2.29.4",
    "mongodb": "^5.7.0",
    "node-cron": "^3.0.2",
    "tslib": "^2.6.0"
  },
  "resolutions": {
    "discord-api-types": "0.37.20"
  },
  "devDependencies": {
    "@sapphire/prettier-config": "^1.4.5",
    "@sapphire/ts-config": "^4.0.0",
    "@types/eslint": "^8.44.0",
    "@types/jsonfile": "^6.1.1",
    "@types/lodash": "^4.14.195",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.4.2",
    "@types/node-cron": "^3.0.8",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  },
  "prettier": "@sapphire/prettier-config",
  "packageManager": "yarn@3.4.1",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#utils/*": "./dist/lib/util/*.js"
  },
  "engines": {
    "node": "^16",
    "npm": "^7"
  }
}
