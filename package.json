{
  "name": "dexcom-mongo-backup-bot",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/dustin-lennon/DexcomMongoBackup",
  "author": "Dustin Lennon",
  "license": "UNLICENSE",
  "keywords": [
    "Mongo",
    "Dexcom",
    "Amazon S3"
  ],
  "scripts": {
    "build": "tsc -b src",
    "dev": "yarn build && yarn start",
    "watch": "tsc -b src -w",
    "watch:start": "tsc-watch -b src --onSuccess \"yarn start \"",
    "clean": "node scripts/build/clean.mjs",
    "start": "node --enable-source-maps dist/index.js",
    "update": "yarn upgrade-interactive",
    "format": "prettier --write \"src/**/*.{js,ts,json}\"",
    "pm2-stop:dev": "pm2 stop dexcomMongoBot",
    "pm2-start:dev": "yarn clean && yarn build && pm2 start ./dist/index.js --name=dexcomMongoBot"
  },
  "dependencies": {
    "@aws-sdk/abort-controller": "^3.272.0",
    "@aws-sdk/client-s3": "^3.281.0",
    "@aws-sdk/lib-storage": "^3.281.0",
    "@sapphire/decorators": "^6.0.0",
    "@sapphire/discord-utilities": "^3.0.0",
    "@sapphire/discord.js-utilities": "6.0.2",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "^4.2.0",
    "@sapphire/plugin-api": "^5.0.1",
    "@sapphire/plugin-editable-commands": "^3.0.0",
    "@sapphire/plugin-logger": "^3.0.1",
    "@sapphire/plugin-scheduled-tasks": "^6.0.0",
    "@sapphire/plugin-subcommands": "^4.0.0",
    "@sapphire/time-utilities": "^1.7.8",
    "@sapphire/type": "^2.3.0",
    "@sapphire/utilities": "^3.11.0",
    "@skyra/env-utilities": "^1.1.0",
    "@skyra/http-framework": "^0.15.2",
    "aws-sdk": "^2.1325.0",
    "discord.js": "^14.7.1",
    "filesize": "^10.0.6",
    "jsonfile": "^6.1.0",
    "lodash": "^4.17.21",
    "mkdirp": "^2.1.3",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "mongodb": "^5.1.0",
    "node-cron": "^3.0.2",
    "tslib": "^2.5.0"
  },
  "resolutions": {
    "discord-api-types": "0.37.20"
  },
  "devDependencies": {
    "@sapphire/prettier-config": "^1.4.5",
    "@sapphire/ts-config": "^3.3.4",
    "@types/eslint": "^8.21.1",
    "@types/jsonfile": "^6.1.1",
    "@types/lodash": "^4.14.191",
    "@types/moment-duration-format": "^2.2.3",
    "@types/node": "^18.14.2",
    "@types/node-cron": "^3.0.7",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.0",
    "typescript": "^4.9.5"
  },
  "prettier": "@sapphire/prettier-config",
  "packageManager": "yarn@3.4.1",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#utils/*": "./dist/lib/util/*.js"
  },
  "engines": {
    "node": "^16",
    "npm": "^7"
  }
}
