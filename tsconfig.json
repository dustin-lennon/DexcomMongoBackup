{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"rootDir": "./src",
		"outDir": "dist",
		"incremental": true,
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"baseUrl": "./",
		"types": ["node"],
		"skipLibCheck": true,
		"strict": false,
		"lib": ["ESNext"],
		"target": "ESNext",
		"esModuleInterop": false
	},
	"include": ["src"],
	"exclude": ["node_modules/*", "dist/*"]
}
